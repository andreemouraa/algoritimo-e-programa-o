questão 8 da prova

inicio 
declare real valorHora, horas Trab
escreva "informe valor por hora"
leia valorHora
escreva "informe o numero de horas trabalhadas"
leia horasTrab
atribua salBrut <- salTotal * valorHora
atribua ir salBruto * 0,11
atribua inss <- salBruto * 0,08
atribua sind <- salBruto 0,5
atribua salTotal <- salBruto - ir - inss - sind
escreva "salario bruto : " + salBruto
escreva "INSS: " + sind
escreva "sindicato: " + sind
escreva "salario liquido : " + salTotal
fim

